mixin navmenu(menu_items)
  
  nav(class="btn--mainmenu", id="mainNav")
    ul
      each item in menu_items
        li
          a(href= item.link)
            =item.title

+navmenu(
  [
    {
      'title':'home',
      'link':'./'
    },
    {
      'title':'posts',
      'link':'./posts.html'
    },
    {
      'title':'page',
      'link':'./page.html'
    },
    {
      'title':'contacto',
      'link':'./contacto.html'
    }
  ]
)
